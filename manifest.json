{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Pimp my Trello",
  "homepage_url": "https://github.com/yairper/pimp-my-trello",
  "author": "Yair Perman",
  "version": "0.0.2",
  "manifest_version": 2,
  "icons": {
  },
   "page_action": {
    "default_icon": {
    }
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": [
        "*://trello.com/*"
      ],
      "js": [
        "src/js/helpers.js",
        "src/js/zepto.min.js",
        "src/js/zepto_extensions.js",
        "src/js/builder.js",
        "src/js/label.js",
        "src/js/tags_list.js",
        "src/js/tags/label_tag.js",
        "src/js/tags/icon_tag.js",
        "src/js/card.js",
        "src/js/cards_list_observer.js",
        "src/js/cards_list_mutations.js",
        "src/js/cards_list.js",
        "src/js/board.js",
        "src/js/content.js",
        "src/js/toggle_menu_icons.js",
        "src/js/inject_fa_font_face.js",
        "src/js/index.js"
      ],
      "css": [
        "src/css/scrum_for_trello_overrides.css",
        "css/style.css",
        "css/font-awesome.min.css"
      ]
    }
  ],

  "web_accessible_resources": [
    "fonts/fontawesome-webfont.woff2"
  ]
}
