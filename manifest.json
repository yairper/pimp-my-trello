{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Trello stuff",
  "homepage_url": "https://github.com/yairper/?",
  "author": "Yair Perman",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "pimp my trello",
  "icons": {
  },
   "page_action": {
    "default_icon": {
    }
  },

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/js/helpers.js",
        "src/js/zepto.min.js",
        "src/js/label.js",
        "src/js/tags_list.js",
        "src/js/tags/label_tag.js",
        "src/js/tags/icon_tag.js",
        "src/js/card.js",
        "src/js/cards_list_mutations.js",
        "src/js/cards_list.js",
        "src/js/board.js",
        "src/js/toggle_menu_icons.js"
      ],
      "css": [
        "css/style.css",
        "css/font-awesome.min.css"
      ]
    }
  ],

  "web_accessible_resources": [
    "fonts/fontawesome-webfont.woff2"
  ]
}
